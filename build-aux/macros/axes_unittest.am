# get them distributed
EXTRA_DIST=$(tests)

# objects and executable get generated by the utexec.sh script
# get them cleaned
ut_objects = $(tests:%.cpp=%.lo)
ut_executables = $(tests:%.cpp=%$(EXEEXT))
MOSTLYCLEANFILES=$(ut_objects) $(ut_executables)

.PHONY: ut
ut:
	$(top_builddir)/bin/utexec.sh --srcdir $(srcdir) $(tests)
